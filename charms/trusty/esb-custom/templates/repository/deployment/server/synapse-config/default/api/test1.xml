<?xml version="1.0" encoding="UTF-8"?>
<api xmlns="http://ws.apache.org/ns/synapse" name="test" context="/test">
   <resource methods="GET" uri-template="/{tool}*">
      <inSequence>
         <log level="custom">
            <property name="query" expression="get-property('query.param.test')"/>
            <property name="tool" expression="get-property('uri.var.tool')"/>
         </log>
         <loopback/>
      </inSequence>
      <outSequence>
         <send/>
      </outSequence>
   </resource>
</api>
