<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="cli_jfed_service"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <inSequence>
         <property name="messageType" value="application/xml" scope="axis2"/>
         <header name="Accept" scope="transport" value="application/xml"/>
         <property name="HTTP_METHOD" value="POST" scope="axis2" type="STRING"/>
         <log level="full"/>
         <send>
            <endpoint>
               <address uri="http://[2001:6a8:1d80:23::141]:8080/cli/tool/jfed-create?async&amp;slice={slice}"
                        format="pox"/>
            </endpoint>
         </send>
      </inSequence>
      <outSequence>
         <log level="full"/>
         <send/>
      </outSequence>
   </target>
</proxy>
