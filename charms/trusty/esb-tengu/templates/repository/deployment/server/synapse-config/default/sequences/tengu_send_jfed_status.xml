<?xml version="1.0" encoding="UTF-8"?>
<sequence xmlns="http://ws.apache.org/ns/synapse"
          name="tengu_send_jfed_status"
          statistics="enable">
   <property name="messageType" value="application/xml" scope="axis2"/>
   <header name="Accept" scope="transport" value="text/xml"/>
   <property name="HTTP_METHOD" value="POST" scope="axis2" type="STRING"/>
   <header xmlns:ns="http://org.apache.synapse/xsd"
           name="To"
           expression="concat('http://localhost:8080/cli/tool/jfed-status?slice=', get-property('slice'))"/>
   <log level="full"/>
   <send/>
</sequence>
